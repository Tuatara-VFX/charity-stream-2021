				float smoothstepResult235 = smoothstep( ( _VolumeMinimum + temp_output_284_0 ) , ( temp_output_284_0 + _VolumeMaximum ) , length( ( WorldPosition - _Center ) ));
				float VolumeMask255 = smoothstepResult235;
				float3 ase_worldNormal = IN.ase_texcoord3.xyz;
