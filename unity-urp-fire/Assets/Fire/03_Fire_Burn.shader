				float smoothstepResult251 = smoothstep( ( temp_output_278_0 + 0.0 ) , ( 1.0 - temp_output_278_0 ) , saturate( ( ( VolumeMask255 * 2.0 ) - triplanar256.x ) ));
				float3 temp_cast_0 = (smoothstepResult251).xxx;
				
				float3 BakedAlbedo = 0;
				float3 BakedEmission = 0;
				float3 Color = temp_cast_0;
				float Alpha = 1;
				float AlphaClipThreshold = 0.5;
				float AlphaClipThresholdShadow = 0.5;

